{
    "builds": [
      {
        "src": "football_predictions/wsgi.py",
        "use": "@vercel/python"
            }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "football_predictions/wsgi.py"
      }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }] 

  

}